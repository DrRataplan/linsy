<system>
    <axiom>A</axiom>
    <grammar type="stochastic">
        <variable symbol="B">BB</variable>
        <variable symbol="A">
            <option weight="10">B[+A]-A</option>
            <option weight="10">BB[+A]-A</option>
            <option>A</option>
        </variable>
        <terminal symbol="["/>
        <terminal symbol="]"/>
        <terminal symbol="+"/>
        <terminal symbol="-"/>
    </grammar>
    <render viewBox="0 0 240 240">
        <state x="120" y="240" velocity="16" orientation="90" angle="45" color="black"/>
    </render>
</system>